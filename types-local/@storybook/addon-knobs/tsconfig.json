{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "dom",
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@storybook/addon-knobs": [
                "storybook__addon-knobs"
            ],
            "@storybook/react": [
                "storybook__react"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "vue.d.ts"
    ]
}